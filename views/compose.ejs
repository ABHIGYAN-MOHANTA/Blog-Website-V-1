<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    
    <form>
        <div class="form-group">
          <label for="postTitle">Title</label>
          <input name="postTitle" type="text" class="form-control" id="postTitle" placeholder="Enter Title">
        </div>
        <div class="form-group">
          <label for="postBody">Content</label>
          <textarea cols="40" rows="5" name="postBody" type="text" class="form-control" id="postBody" placeholder="Enter Content"></textarea>
        </div>
        <button type="submit" class="btn btn-primary" name="button">Publish</button>
      </form>
      
</form>

<%- include("partials/footer") -%>